{
  "name": "bremertown_chat",
  "version": "0.0.1",
  "description": "Super simple online chatroom.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nothingworksright/bremertown_chatroom.git"
  },
  "main": "app.js",
  "author": "jmg1138 https://github.com/jmg1138",
  "license": "MIT",
  "readme": "README.md",
  "engines": {
    "node": "8.3.0"
  },
  "dependencies": {
    "express": "4.16.2",
    "helmet": "3.9.0",
    "pug": "2.0.0-rc.4",
    "socket.io": "2.0.4"
  },
  "devDependencies": {
    "codecov": "3.0.0",
    "env-test": "1.0.0",
    "eslint": "4.9.0",
    "intercept-stdout": "0.1.2",
    "mocha": "3.5.0",
